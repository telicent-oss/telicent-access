{
  "name": "telicent-access-api",
  "version": "1.0.0",
  "description": "Project to provide user attribute lookups",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --ignore ./cognito-local --exec babel-node src/index.js",
    "build": "babel src --out-dir dist --copy-files",
    "prepare": "husky install",
    "postinstall": "cd frontend && yarn install",
    "test": "jest --no-cache --coverage=false --watchAll=false && cd frontend && yarn test --coverage=false --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Telicent-io/telicent-access.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Telicent-io/telicent-access/issues"
  },
  "homepage": "https://github.com/Telicent-io/telicent-access#readme",
  "dependencies": {
    "@babel/runtime": "^7.24.1",
    "aws-sdk": "^2.1584.0",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "express": "^4.19.1",
    "jws": "^4.0.0",
    "mongoose": "^6.12.7",
    "papaparse": "^5.4.1",
    "scim2-parse-filter": "^0.2.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^6.4.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "mockingoose": "^2.16.2",
    "msw": "^1.3.3",
    "nodemon": "^2.0.22",
    "release-please": "^14.17.5",
    "webpack": "^5.91.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/**/*.test.js"
    ],
    "setupFiles": [
      "<rootDir>/setupEnvs.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!node_modules",
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/mocks",
      "src/router",
      "src/lib",
      "src/index.js",
      "src/database"
    ]
  }
}
